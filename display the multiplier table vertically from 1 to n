#include <stdio.h>

int main() {
    int i,j, n,result;
     printf("enter the Table to be calculated: ");
    scanf("%d",&n);
     for(i=1; i<=10; i++){
         for( j=1; j<=n;j++){
             result= i*j;
             printf("%d X %d = %d\n", i,j,result);
         }
          
     }
    
      
     

    return 0;
}
